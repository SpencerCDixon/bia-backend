{
  "name": "bia-backend",
  "version": "0.0.1",
  "description": "Backend API for personal productivity CLI bia",
  "main": "index.js",
  "scripts": {
    "babel": "babel src -d dist",
    "build": "npm run lint && npm run clean && npm run babel",
    "clean": "rimraf dist",
    "start": "nf start -w --procfile Procfile.dev",
    "db": "mongod --dbpath=./data --port 27017",
    "lint": "eslint ./src",
    "production": "node dist/index.js",
    "server": "nodemon src/index.js --exec babel-node",
    "seed": "babel-node scripts/seed_data.js",
    "seed:prod": "babel-node scripts/seed_data_prod.js",
    "test": "NODE_ENV=test mocha --compilers js:babel-register --require ./tests/test-bundle.js --recursive ./tests/**/*.js --full-trace"
  },
  "author": "Spencer Dixon",
  "license": "MIT",
  "engines": {
    "node": "5.7.0",
    "npm": "3.6.0"
  },
  "dependencies": {
    "babel-runtime": "^6.9.2",
    "co-body": "^4.0.0",
    "co-monk": "^1.0.0",
    "koa": "^1.2.0",
    "koa-basic-auth": "^1.1.2",
    "koa-body": "^1.4.0",
    "koa-logger": "^1.3.0",
    "koa-router": "^5.4.0",
    "koa-validate": "^0.2.11",
    "mongodb": "^1.4.40",
    "mongoskin": "^1.3.20",
    "monk": "^1.0.1",
    "scd-fp-util": "^0.1.0",
    "dotenv": "^2.0.0"
  },
  "devDependencies": {
    "axios": "^0.12.0",
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-plugin-resolver": "0.0.7",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "csv-parse": "^1.1.1",
    "eslint": "^2.3.0",
    "eslint-plugin-babel": "^3.1.0",
    "foreman": "^1.4.1",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.1",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "supertest": "^1.2.0"
  }
}
